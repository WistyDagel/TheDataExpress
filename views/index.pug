extends layout 

block content
    h1 #{title}

    form(id='form' method='post' action='/home')
        p #{username}: 
            input(type='text' name='username' autocomplete='off')
        p #{password}: 
            input(type='password' name='password' autocomplete='off')
        p
            input(id='btn' type='submit' value='Log In')

    a(href=config['menu'][1][1])
        div(class='create_account') #{noAccount}